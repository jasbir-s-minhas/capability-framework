<?xml version="1.0" encoding="UTF-8"?>
<capabilityConfig  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="test.xsd">
    <version>1.1</version>
    <currentProvince>ON</currentProvince>
    <runMode>DEV</runMode>
    <!-- A test group for testing the capability frame. Meant to be used for testing and an example only. -->
    <capabilityGroup name="testGroup1L1" enabled="true">
        <capabilityGroup name="testGroup1L2" enabled="true">
            <capability>
                <name>testStringCapability</name>
                <values type="string">
                    <province></province>
                </values>
            </capability>
            <capability>
                <name>testBooleanCapability</name>
                <value type="boolean">true</value>
            </capability>
            <capability>
                <name>testIntCapability</name>
                <value type="int">999</value>
            </capability>
            <capability>
                <name>testFloatCapability</name>
                <value type="float">10.9</value>
            </capability>
        </capabilityGroup>
    </capabilityGroup>
</capabilityConfig>